{% extends "AppBundle:Account:templates/memberList.html.twig" %}

{% block userList %}
<div class="user-list-wrap">
    <ul class="user-list-organization">
        <li class="active"><span>{{'business_community.Members'|trans}} </span><strong class="number">({{ pagination.getTotalItemCount }})</strong></li>
        <li><a href=""class="tab-search">Search</a></li>
    </ul>
    
    {% for item in pagination %}
        {% include 'AppBundle:Account:parts/userListItem.html.twig' with {'user': item }%}
    {% endfor %}
    
    {{ knp_pagination_render(pagination) }}
</div>
{% endblock userList %}